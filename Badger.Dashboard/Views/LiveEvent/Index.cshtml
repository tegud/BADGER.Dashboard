@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Layouts/_Layout.cshtml";
}

<div>
    <form id="queryoptions" class="queryoptions">
        Query:
        <input type="text" name="query" class="query" value="lr_errors(source_host,pagetype,language,Exception)" list="predefinedqueries"></input>
        <input type="submit" value="Watch Events"></input>
        <datalist id="predefinedqueries">
            <option value="lr_errors(source_host,pagetype,language,Exception)"></option>
            <option value="domain_events(commission, nights, rooms, affiliateName, hotelProvider, commissionValue, bookerCountry)"></option>
            <option value="lr_errors(source_host,pagetype,language,Exception)"></option>
            <option value="hotel_acquisitions_errors(source_host,Exception))"></option>
        </datalist>
    </form>
</div>
<div>
    <table class="eventstable">
        <thead></thead>
        <tbody></tbody>
    </table>
</div>


@section Stylesheet
{
    <link rel="stylesheet" href="~/Assets/css/liveevents.css" />
}

@section Javascript
{
    <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js"></script>
    <script src="~/Assets/js/liveevents.js" type="text/javascript"></script>
}